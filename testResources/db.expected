DROP TABLE IF EXISTS user;
CREATE TABLE user(id bigint(10) primary key,username varchar(100),password varchar(100));
DROP TABLE IF EXISTS userlog;
CREATE TABLE userlog(id bigint(10) primary key auto_increment,username varchar(100),status varchar(100));
DROP TABLE IF EXISTS country;
CREATE TABLE country(id bigint(10) primary key,continentId bigint(10),name varchar(100),language varchar(100),capital varchar(100));
DROP TABLE IF EXISTS state;
CREATE TABLE state(id bigint(10) primary key,countryId bigint(10),name varchar(100));
