DROP TABLE IF EXISTS user;
CREATE TABLE user(id int primary key,username varchar(100),password varchar(100));
DROP TABLE IF EXISTS userlog;
CREATE TABLE userlog(id int primary key auto_increment,username varchar(100),status varchar(100));
DROP TABLE IF EXISTS country;
CREATE TABLE country(id int primary key,continentId int,name varchar(100),language varchar(100),capital varchar(100));
DROP TABLE IF EXISTS state;
CREATE TABLE state(id int primary key,countryId int,name varchar(100));
