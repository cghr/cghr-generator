{
"schemaName":"{{schemaName}}",
"onSave":"{{onSave}}",
"properties":[
{{#each properties}}

        {
        "name":"{{name}}",
        "type":"{{type}}",
        "valdn":"{{valdn}}",
        "label":"{{label}}",
        "flow":"{{flow}}",
        "value":"{{value}}",
        "items":[
        {{#each items}}
            {{#unless @last}}
                {"text":"{{text}}","value":"{{value}}"},
            {{else}}
                {"text":"{{text}}","value":"{{value}}"}
            {{/unless}}
        {{/each}}
        ],
        "image":"{{image}}",
        "lookup":{
        {{#with lookup}}
            "lookup_entity":"{{lookup_entity}}",
            "lookup_field":"{{lookup_field}}",
            "lookup_ref":"{{lookup_ref}}"
        {{/with}}
        }

        }{{~#unless @last ~}},
 {{/unless}}

{{/each}}

]
}