{
"onSave":"{{onSave}}",
"properties":[
{{#each properties}}
    {{#unless @last}}
        {
        "name":"{{name}}",
        "type":"{{type}}",
        "valdn":"{{valdn}}",
        "label":"{{label}}",
        "flow":"{{flow}}",
        "value":"{{value}}",
        "items":[
        {{#each items}}
            {{#unless @last}}
                {"text":"{{text}}","value":"{{value}}"},
            {{else}}
                {"text":"{{text}}","value":"{{value}}"}
            {{/unless}}
        {{/each}}
        ]
        },
    {{else}}
        {
        "name":"{{name}}",
        "type":"{{type}}",
        "valdn":"{{valdn}}",
        "label":"{{label}}",
        "flow":"{{flow}}",
        "value":"{{value}}",
        "items":[
        {{#each items}}
            {{#unless @last}}
                {"text":"{{text}}","value":"{{value}}"},
            {{else}}
                {"text":"{{text}}","value":"{{value}}"}
            {{/unless}}
        {{/each}}
        ]
        }
    {{/unless}}
{{/each}}

]
}