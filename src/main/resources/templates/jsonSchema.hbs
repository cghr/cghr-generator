{
"schemaName":"{{schemaName}}",
"onSave":"{{onSave}}",
"properties":[
{{#each properties}}

        {
        "name":"{{name}}",
        "type":"{{type}}",
        "valdn":"{{valdn}}",
        "label":"{{label}}",
        "flow":"{{flow}}",
        "value":"{{value}}",
        "items":[
        {{#each items}}
            {"text":"{{text}}","value":"{{value}}"}
            {{~#unless @last ~}},{{/unless}}
        {{/each}}
        ],
        "image":"{{image}}",
        "lookup":{
        {{#with lookup}}
            "entity":"{{entity}}",
            "field":"{{field}}",
            "ref":"{{ref}}"
        {{/with}}
        },
        "crossFlow":[
        {{#each crossFlow}}
        {"entity":"{{entity}}","field":"{{field}}","ref":"{{ref}}","condition":"{{condition}}"}
        {{~#unless @last ~}},{{/unless}}
        {{/each}}
        ],
        "crossCheck":{
        {{#with crossCheck}}
            "entity":"{{entity}}",
            "field":"{{field}}",
            "ref":"{{ref}}",
            "condition":"{{condition}}"
        {{/with}}
        }
        }

        {{~#unless @last ~}},{{/unless}}

{{/each}}

]
}