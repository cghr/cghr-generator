{{~#each entities ~}}
#Table:{{name}}
DROP TABLE IF EXISTS `{{name}}`;
CREATE TABLE `{{name}}`(
{{~#each properties ~}}
    {{~#unless @last ~}}
        {{name}} {{type}} {{key}} {{strategy}},
    {{~else ~}}
        {{name}} {{type}} {{key}} {{strategy}}
    {{~/unless ~}}
{{~/each ~}}
);
{{~/each ~}}
